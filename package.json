{
  "name": "react.cordova.template",
  "displayName": "HelloCordova",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "chmod": "chmod 755 ./bat/*",
    "build": "bat/build.sh",
    "build--en": "bat/build.sh en",
    "checkkey": "bat/checkkey.sh",
    "createkey": "bat/createkey.sh",
    "emulate--android": "npm run build && cordova emulate android",
    "emulate--android--en": "npm run build--en && cordova emulate android",
    "emulate--ios": "npm run build && cordova emulate ios",
    "emulate--ios--en": "npm run build--en && cordova emulate ios",
    "release--android": "npm run build && bat/release-android.sh ja",
    "release--android--en": "npm run build--en && bat/release-android.sh en",
    "release--ios": "npm run build && bat/release-ios.sh ja",
    "release--ios--en": "npm run build--en && bat/release-ios.sh en"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "cordova-admob-sdk": "^0.24.1",
    "cordova-plugin-whitelist": "^1.3.4"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-admob-sdk": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  },
  "dependencies": {
    "cordova-android": "9.0.0",
    "cordova-ios": "6.1.1",
    "xml2js": "^0.4.23"
  }
}
