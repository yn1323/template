{
  "name": "svelte-template-sass-webpack",
  "version": "1.0.0",
  "description": "Svelte Boiler Template include Sass webpack",
  "scripts": {
    "build": "prettier --write './src/**/*.{js,scss,svelte}' && cross-env NODE_ENV=production webpack",
    "dev": "webpack-dev-server --watch-content-base --content-base public",
    "lint": "prettier --write './src/**/*.{js,scss,svelte}'",
    "test": "jest --watchAll",
    "coverage": "jest --watchAll --coverage",
    "cypress": "cypress open",
    "test--prep": "webpack-dev-server --watch-content-base --content-base public --port 8095",
    "test--e2e": "cypress run"
  },
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cypress": "^3.8.2",
    "eslint": "^6.7.2",
    "eslint-plugin-svelte3": "^2.7.3",
    "jest": "^24.9.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "sass-loader": "^8.0.0",
    "serve": "^11.0.0",
    "start-server-and-test": "^1.10.7",
    "style-loader": "^0.23.1",
    "svelte": "^3.0.0",
    "svelte-loader": "2.13.3",
    "svelte-preprocess": "^3.3.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yn1323/svelte-template-webpack-sass.git"
  },
  "keywords": [
    "Svelte",
    "Template",
    "Sass",
    "webpack"
  ],
  "author": "ynatani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yn1323/svelte-template-webpack-sass/issues"
  },
  "homepage": "https://github.com/yn1323/svelte-template-webpack-sass#readme",
  "dependencies": {}
}
