{
  "name": "gatsby-capacitor-template",
  "version": "0.0.1",
  "description": "Gatsby Capacitor Template",
  "private": true,
  "scripts": {
    "start": "gatsby clean && gatsby develop",
    "serve": "gatsby clean && gatsby serve",
    "build": "gatsby clean && OUTPUT_DIR=build gatsby build",
    "start--en": "REACT_APP_LANG=en gatsby develop",
    "build--en": "REACT_APP_LANG=en gatsby build --mode production",
    "lint": "eslint --ext ./ 'src/**/*.{js,jsx,ts,tsx}';",
    "i18n-extract": "NODE_ENV=development babel './src/**/*.{js,jsx,ts,tsx}'",
    "chmod": "chmod 755 ./bat/*",
    "checkkey": "bat/checkkey.sh",
    "createkey": "bat/createkey.sh",
    "release--android": "npm run build && bat/release-android.sh ja",
    "release--android--en": "npm run build--en && bat/release-android.sh en",
    "sign--android": "bat/sign-android.sh",
    "start:android": "npm run build && npx cap copy android && npx cap open android",
    "start:ios": "npm run build && npx cap copy ios && npx cap open ios"
  },
  "keywords": [
    "gatsby",
    "capacitor"
  ],
  "author": "yn1323 <yn1323@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.1",
    "axios": "^0.21.0",
    "gatsby": "^3.4.1",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.2",
    "uuid": "^3.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@capacitor/android": "^2.4.7",
    "@capacitor/cli": "^2.4.7",
    "@capacitor/core": "^2.4.7",
    "@capacitor/ios": "^2.4.7",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.6",
    "@types/redux-logger": "^3.0.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-plugin-i18next-extract": "^0.8.2",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-gatsby-cloud": "^2.4.1",
    "gatsby-plugin-image": "^1.4.0",
    "gatsby-plugin-manifest": "^3.4.0",
    "gatsby-plugin-offline": "^4.4.0",
    "gatsby-plugin-output": "^0.1.3",
    "gatsby-plugin-react-helmet": "^4.4.0",
    "gatsby-plugin-sass": "^4.5.0",
    "gatsby-plugin-sharp": "^3.4.1",
    "gatsby-source-filesystem": "^3.4.0",
    "gatsby-transformer-sharp": "^3.4.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "redux-logger": "^3.0.6",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "i18next-extract",
        {
          "locales": [
            "ja",
            "en"
          ],
          "outputPath": "./src/locales/{{locale}}.json"
        }
      ]
    ]
  }
}
